function [ output_args ] = simulateWithLagrangian( L, theta, thetaD, deltaT )
%SIMULATEWITHLAGRANGIAN Summary of this function goes here
%   Detailed explanation goes here

[eqn, thetaDD] = solveEulerLagrange(L, theta, thetaD, t);

disp('Solving for thetaDD...');
[A,b] = equationsToMatrix(eqn, thetaDD);
A = simplify(A);
b = simplify(b);


end

